//unsigned char LED_OFF[1024]={};


void sdelay(unsigned int t)
{
  int i,j;
for(i=0;i<t;i++)
for(j=0;j<10;j++);
}
void ldelay(unsigned int t)
{
  int i,j;
for(i=0;i<t;i++)
for(j=0;j<5000;j++);
}

void LCD_strobe(void)
{
IOSET1=EN;
sdelay(5);
IO1CLR=EN;
sdelay(5);
}

void GLCD_data (unsigned char ch)
{
IO1CLR=LCD_PORT;
IO1SET=ch<<LCD_SHIFT;
    IOSET1=RS;
LCD_strobe();

}
void GLCD_cmd(unsigned char ch)
{
IO1CLR=LCD_PORT;
IO1SET=ch<<LCD_SHIFT;
IO1CLR=RS;
LCD_strobe();
}

void GLCD_init()
{
int i;
PINSEL0=0;
PINSEL1=0;
PINSEL2=0;

IODIR1=LCD_PORT|RS|EN|CS1|CS2|GRST|RW;
IOSET1=GRST|CS1|CS2;
IOCLR1=RW|RS|EN;
for(i=0;i<10;i++)
{
GLCD_cmd(0x3F);
GLCD_cmd(0x40);
GLCD_cmd(0xB8);

}
}

void GLCD_display(unsigned char * temp1)
{
int i,j;
for(i=0;i<8;i++)
{
IOSET1=CS1;
IO1CLR=CS2;
GLCD_cmd(0XB8 | i);
GLCD_cmd(0X40);
for(j=0;j<64;j++)
GLCD_data(temp1[(i*128)+j]);
IOCLR1=CS1;
IO1SET=CS2;
GLCD_cmd(0XB8 | i);
GLCD_cmd(0X40);
for(j=0;j<128;j++)
GLCD_data(temp1[(i*128)+j]);
}
}

int main()
{
//int temp;
GLCD_init();
//GLCD_display(temp);
ldelay(4000);
while(1)
{
//GLCD_display(LED_OFF);
//ldelay(2000);
GLCD_display(LED_ON);
ldelay(2000);
}
return 0;
}
